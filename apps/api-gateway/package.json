{
  "name": "@multivendor-ecommerce/api-gateway",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@multivendor-ecommerce/api-gateway:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@multivendor-ecommerce/api-gateway:build:development"
          },
          "production": {
            "buildTarget": "@multivendor-ecommerce/api-gateway:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "express-http-proxy": "^2.1.1",
    "express-rate-limit": "^7.5.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express-http-proxy": "^1.6.6",
    "@types/morgan": "^1.9.9",
    "@types/swagger-ui-express": "^4.1.8"
  }
}

{
  "name": "@repo/server",
  "version": "1.0.0",
  "description": "",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.js"
    },
    "./middlewares/*": {
      "import": "./dist/middlewares/*.js",
      "require": "./dist/middlewares/*.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc --build && npx tsc --project tsconfig.cjs.json",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "http-status-codes": "2.3.0",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@types/express": "5.0.1"
  },
  "peerDependencies": {
    "express": "5.1.0"
  }
}
